{# Resources/views/Wiki/twig-extension/typography/small.html.twig #}
{% extends "@Bootstrap/Wiki/layout.html.twig" %}
{% import "@Bootstrap/Wiki/macros.html.twig" as wiki %}
{% block bootstrapWikiContent %}
    {{ wiki.functionsTitle() }}
    {{ wiki.functionsList([
        "bootstrapSmall",
    ]) }}
    <div class="panel panel-default">
        {{ wiki.functionTitle("bootstrapSmall") }}
        <div class="panel-body">
            {{ wiki.argumentsList([
                ["string", "content", "the content"],
            ])}}
            {{ wiki.functionExample('{{ bootstrapSmall({"content": "content"}) }}') }}
            {{ wiki.functionOutput(bootstrapSmall({"content": "content"})) }}
        </div>
    </div>
{% endblock %}
